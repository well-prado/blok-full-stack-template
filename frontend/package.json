{
  "name": "modern-react-template",
  "version": "1.0.0",
  "description": "Modern React template with React 19, Tailwind CSS v4, and Shadcn UI",
  "type": "module",
  "scripts": {
    "dev": "npm run blok:codegen && concurrently \"npm run blok:watch\" \"vite\"",
    "dev:nowatch": "npm run blok:codegen && vite",
    "build": "npm run blok:codegen && vite build",
    "build:fresh": "npm run blok:codegen && tsc && vite build",
    "build:analyze": "npm run blok:codegen && tsc && ANALYZE=true vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "blok:codegen": "cd .. && npx blok-codegen-auto",
    "blok:watch": "cd .. && npx blok-codegen-auto --watch",
    "blok:validate": "blok-codegen validate --url http://localhost:4000"
  },
  "dependencies": {
    "@well-prado/blok-typed-sdk": "workspace:*",
    "@well-prado/blok-front-sdk-core": "^1.0.2",
    "@formkit/auto-animate": "^0.9.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.0.0",
    "@well-prado/blok-react-sdk": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.16",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tw-animate-css": "^1.3.8",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@well-prado/blok-codegen": "^1.0.7",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "4.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.20"
  }
}
